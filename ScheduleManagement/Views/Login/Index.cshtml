    <script>
        function login() {
            var userName = $('#Employee_UserName').val();
            var password = $('#Employee_Password').val();
            var department = $('#Employee_Department').val();
            var data = {
                Employee_UserName: userName,
                Employee_Password: password,
                Employee_Department: department
            }
            // var serialize = $('#login').serialize();
            $.post("/Login/LoginData", data, function (res) {
                console.log(res);
                if (res.FirstTimeLoginStatus == 'False') {
                    popUp();
                    $('#EmpID').val(res.Employee_Id);
                    $('#EmpPassword').val(res.Employee_Password);
                    bootbox.confirm({
                        size: "small",
                        message: "Are you sure you want to Login?",
                        callback: function (result) {
                            if (result == true) {
                                popUp();
                                $('#EmpID').val(res.Employee_Id);
                                $('#EmpPassword').val(res.Employee_Password);
                            }
                            /* result is a boolean; true = OK, false = Cancel*/
                        }
                    })
                }
                else {
                    window.location.href = "@Url.Action("Index", "Home")";
                }
            })
        }

        //if (res.FirstTimeLoginStatus == 'FALSE') {
        //    $('#LoginConfirmModal').modal('toggle');
        //    $('#EmpID').val(res.ID);
        //}

        function PasswordClear() {
            $('#OldPassword').val("");
            $('#NewPassword').val("");
            $('#ConfirmPssword').val("");
        }
        function hideLogin() {
            $('#LoginForm').hide();
            $('#ChangePassword').show();
        }
        function popUp() {
            $('#forget-password').click();

            //$('#LoginConfirmModal').modal('toggle');
            //$('#ChangePasswordModal').modal('toggle');
            PasswordClear();
        }

        function ShowLogin() {
            $('#back-btn').click();

           // $('#LoginForm').show();
        }
        //Validation Function
        function Validation() {
            var isValid = true;
            var Emppassword = $('#EmpPassword').val();
            var OldPassword = $('#OldPassword').val();
            var password = $('#NewPassword').val();
            var confirmpassword = $('#ConfirmPssword').val();

            if ($('#OldPassword').val().trim() == "") {
                $('#OldPassword_error').show();
                $("#OldPassword_error").fadeOut(2000);
                isValid = false;
            }
            else if (OldPassword != Emppassword) {
                $('#OldPassword_error_Val').show();
                $("#OldPassword_error_Val").fadeOut(2000);
            } else {
                $('#OldPassword_error').hide();
                $('#OldPassword_error_Val').hide();
            }
            if ($('#NewPassword').val().trim() == "") {
                $('#NewPassword_error').show();
                $("#NewPassword_error").fadeOut(2000);
                isValid = false;
            }
            else {
                $('#NewPassword_error').hide();
            }
            if ($('#ConfirmPssword').val().trim() == "") {
                $('#ConfirmPssword_error').show();
                $("#ConfirmPssword_error").fadeOut(2000);
                isValid = false;
            } else if (confirmpassword != password) {
                $('#ConfirmPssword_error_Val').show();
                $("#ConfirmPssword_error_Val").fadeOut(2000);
            } else {
                $('#ConfirmPssword_error').hide();
                $('#ConfirmPssword_error_Val').hide();
            }
            return isValid;
        }

        function viewPassword(id, toggle) {
            var passwordInput = document.getElementById(id);
            var passStatus = document.getElementById(toggle);

            if (passwordInput.type == 'password') {
                passwordInput.type = 'text';
                passStatus.className = 'fa fa-eye-slash';
            }
            else {
                passwordInput.type = 'password';
                passStatus.className = 'fa fa-eye';
            }

        }
        function ChangPassword() {
            var res = Validation();
            if (res == false) {
                return false;
            }
            var EmpId = $('#EmpID').val();
            var password = $('#NewPassword').val();

            var data = {
                Employee_Id: EmpId,
                Employee_Password: password,
                FirstTimeLoginStatus: "True"
            }
            console.log(data);
            $.post("/Login/ChangePasswordEmployee", data, function (res) {
                ShowLogin();
                PasswordClear();
                $('#Employee_Name').val("");
                $('#Employee_Password').val("");
                $("#Password-alert").fadeTo(2000, 500).slideUp(500, function () {
                    $("#Password-alert").slideUp(500);
                });

            });
        }
    </script>
